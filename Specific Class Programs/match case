name = "Thomas"

from datetime import datetime

start = datetime.now()
if name== "Thomas":
  print(name)
elif name == "Michael":
  print(name)
else:
  print("not an option")
print(datetime.now()-start)


start = datetime.now()
match name:
  case "Thomas":
    print(name)
  case "Michael":
    print(name)
  case _:
    print("Not an option")

print(datetime.now()-start)
